{
    "name": "mileschou/testrails",
    "description": "Testrail SDK",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.3 | ^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^9.12"
    },
    "require-dev": {
        "laravel-zero/framework": "^9.1",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "MilesChou\\Testrails\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "bin": ["testrails"]
}
